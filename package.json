{
  "name": "kanban-light",
  "version": "0.0.1",
  "description": "A light-weight Kanban application",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Firef0x/kanban-light.git"
  },
  "keywords": [
    "Kanban"
  ],
  "author": "Firef0x",
  "license": "GPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/Firef0x/kanban-light/issues"
  },
  "homepage": "https://github.com/Firef0x/kanban-light#readme",
  "engines": {
    "node": ">= 8.0.0",
    "npm": ">= 5.0.0"
  },
  "scripts": {
    "build": "npm run clean && npm run copyStatic && cross-env NODE_ENV=production webpack --progress --config config/webpack.prod.js",
    "clean": "rimraf public",
    "copyStatic": "cpx \"static/**/*.*\" public/",
    "dev": "npm run copyStatic && cross-env NODE_ENV=development NPCONFIG=dev node --inspect src/server/www",
    "lint": "eslint --ext .js,.jsx -f ./node_modules/eslint-html-reporter/reporter.js -o lint-report.html src",
    "start": "cross-env NODE_ENV=production node src/server/www",
    "startProd": "npm run copyStatic && cross-env NODE_ENV=production node src/server/www",
    "test": "node config/test.js",
    "test:cover": "npm test -- --coverage",
    "test:update": "npm test -- --updateSnapshot"
  },
  "browserslist": [
    "> 0.2%",
    "not dead",
    "not ie <= 8",
    "not op_mini all"
  ],
  "dependencies": {
    "core-js": "^3.8.1",
    "dompurify": "^2.2.4",
    "express": "^4.17.3",
    "hbs": "^4.1.1",
    "immutability-helper": "^3.1.1",
    "json-stringify-safe": "^5.0.1",
    "lodash.throttle": "^4.1.1",
    "marked": "^1.2.7",
    "prop-types": "^15.7.2",
    "react": "^16.14.0",
    "react-dnd": "^11.1.3",
    "react-dnd-html5-backend": "^11.1.3",
    "react-dom": "^16.14.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-transition-group": "^4.4.1",
    "redux": "^4.0.5",
    "redux-thunk": "2.3.0",
    "reselect": "^4.0.0",
    "serve-favicon": "^2.5.0",
    "whatwg-fetch": "^3.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.6.3",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.3",
    "@babel/preset-react": "^7.6.3",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "autoprefixer": "^9.8.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.9.0",
    "babel-loader": "^8.0.6",
    "babel-preset-jest": "^24.9.0",
    "chai": "^4.2.0",
    "cross-env": "^7.0.3",
    "css-loader": "^2.0.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-html-reporter": "^0.7.4",
    "eslint-plugin-flowtype": "^2.50.3",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "2.2.0",
    "file-loader": "^4.2.0",
    "html-webpack-plugin": "^3.2.0",
    "http-server": "^0.12.3",
    "husky": "^3.0.8",
    "jest": "^24.9.0",
    "jest-enzyme": "^7.1.1",
    "jest-html-reporter": "^2.5.0",
    "jsdom": "^13.1.0",
    "less": "^3.10.3",
    "less-loader": "^5.0.0",
    "lint-staged": "^10.5.3",
    "mini-css-extract-plugin": "^0.5.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "react-hot-loader": "^4.12.15",
    "rimraf": "^3.0.2",
    "style-loader": "^1.0.0",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.3",
    "webpack-hot-middleware": "^2.25.0"
  }
}
